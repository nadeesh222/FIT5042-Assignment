<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    
    <navigation-rule>
        <from-view-id> donor_display.xhtml </from-view-id>
        <navigation-case>
            <from-outcome>addDon</from-outcome>
                    <to-view-id>
                        admin/donor_add.xhtml
                    </to-view-id>
            <redirect></redirect>
        </navigation-case>
    </navigation-rule>
    
    
    
    <navigation-rule>
        <from-view-id> project_display.xhtml </from-view-id>
        <navigation-case>
            <from-outcome>addProj</from-outcome>
                    <to-view-id>
                      admin/project_add.xhtml
                    </to-view-id>
            <redirect></redirect>
        </navigation-case>
    </navigation-rule>
    
</faces-config>
