<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

     <navigation-rule>
        <from-view-id> project_display.xhtml </from-view-id>
        <navigation-case>
            <from-outcome>projsearch</from-outcome>
                    <to-view-id>
                      project_search.xhtml
                    </to-view-id>
            <redirect></redirect>
        </navigation-case>
    </navigation-rule>
     
    
    
    
     <navigation-rule>
        <from-view-id> /donor_display.xhtml </from-view-id>
        <navigation-case>
            <from-outcome>donManage</from-outcome>
                    <to-view-id>
                      admin/donor_manage.xhtml
                    </to-view-id>
            <redirect></redirect>
        </navigation-case>
    </navigation-rule>
    
    
    <navigation-rule>
        <from-view-id> admin/donor_manage.xhtml </from-view-id>
        <navigation-case>
            <from-outcome>addDon</from-outcome>
                    <to-view-id>
                        admin/donor_add.xhtml
                    </to-view-id>
            <redirect></redirect>
        </navigation-case>
    </navigation-rule>
    
    
    
    
      
    <navigation-rule>
        <from-view-id> admin/donor_add.xhtml </from-view-id>
        <navigation-case>
            <from-outcome>domDisplay1</from-outcome>
                    <to-view-id>
                        common/donor_display.xhtml
                    </to-view-id>
            <redirect></redirect>
        </navigation-case>
    </navigation-rule>
    
    
    
    
    
    <navigation-rule>
        <from-view-id> project_display.xhtml </from-view-id>
        <navigation-case>
            <from-outcome>proj_manage</from-outcome>
                    <to-view-id>
                     admin/project_manage.xhtml
                    </to-view-id>
            <redirect></redirect>
        </navigation-case>
    </navigation-rule>
    
    
    
     <navigation-rule>
        <from-view-id>  admin/project_manage.xhtml </from-view-id>
        <navigation-case>
            <from-outcome>proj_add</from-outcome>
                    <to-view-id>
                      admin/project_add.xhtml
                    </to-view-id>
            <redirect></redirect>
        </navigation-case>
    </navigation-rule>
    
    
   
</faces-config>
